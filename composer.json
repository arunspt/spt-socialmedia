{
    "name": "spt/spt-socialmedia",
    "type": "typo3-cms-extension",
    "description": "Extension to manage social media widgets.",
    "authors": [
        {
            "name": "Arun Chandran",
            "role": "Product Manager"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^12",
        "php": "^8.1 || ^8.2 || ^8.3"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/arunspt/spt-socialmedia"
      }
    ],
    "autoload": {
        "psr-4": {
            "SPT\\SptSocialmedia\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/spt-socialmedia": "self.version"
    },
    "extra": {
      "typo3/cms": {
         "extension-key": "spt_socialmedia"
      }
   }
}